{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environmentName": {
      "value": "${AZURE_ENV_NAME}"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "azureOpenAIEndpoint": {
      "value": "${AZURE_OPENAI_ENDPOINT}"
    },
    "gptDeployment": {
      "value": "${AZURE_OPENAI_DEPLOYMENT=gpt-4o}"
    },
    "dalleDeployment": {
      "value": "${AZURE_OPENAI_DALLE_DEPLOYMENT=dall-e-3}"
    },
    "newsApiKey": {
      "value": "${NEWS_API_KEY=}"
    },
    "gnewsApiKey": {
      "value": "${GNEWS_API_KEY=}"
    },
    "newsDataApiKey": {
      "value": "${NEWSDATA_API_KEY=}"
    },
    "instagramUsername": {
      "value": "${INSTAGRAM_USERNAME=}"
    },
    "instagramPassword": {
      "value": "${INSTAGRAM_PASSWORD=}"
    }
  }
}
