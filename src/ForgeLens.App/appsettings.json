  "Instagram": {
    "Username": "",
    "Password": "",
    "EnableFilters": false,
    "DefaultHashtags": ["#ai", "#aiart", "#trending", "#viral"],
    "MaxHashtags": 30,
    "PostingDelayMinutes": 30
  },
  "HeadlessMode": false,
  "VerboseLogging": false,
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
