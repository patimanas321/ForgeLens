{
  "AzureOpenAI": {
    "Endpoint": "",
    "DeploymentGPT4o": "gpt-4o",
    "DeploymentDallE": "dall-e-3",
    "UseAzureIdentity": true
  },
  "Foundry": {
    "ProjectEndpoint": "",
    "ModelDeploymentName": ""
  },
  "TrendSources": {
